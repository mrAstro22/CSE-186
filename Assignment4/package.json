    "strip-comments": "*",
    "vite": "*",
    "vite-plugin-eslint": "*",
    "vitest": "*"
  },
  "scripts": {
    "build": "vite build",
    "preview": "vite preview",
    "dev": "vite --port 3000",
    "t2e": "vitest run --reporter=verbose --no-file-parallelism --testTimeout=20000 --exclude 'src/__tests__/*'",
    "e2e": "concurrently --kill-others \"npm run preview\" \"npm run t2e\"",
    "b2e": "(npm run build) && (npm run e2e)",
    "lint": "eslint src --no-inline-config --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run --reporter=verbose --coverage --exclude 'e2e/*'",
    "watch": "vitest",
    "zip": "bestzip CSE186.Assignment4.Submission.zip src/*"
  }
