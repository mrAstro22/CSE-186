    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-router-dom": "*",
    "uuid": "*"
  },
  "devDependencies": {
    "@eslint/compat": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "@vitejs/plugin-react": "*",
    "@vitest/coverage-v8": "*",
    "bestzip": "*",
    "concurrently": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-jsdoc": "*",
